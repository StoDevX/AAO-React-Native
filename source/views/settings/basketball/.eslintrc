{
  extends: 'airbnb',
  plugins: [
    'react',
    'jsx-a11y'
  ],
  rules: {
    "react/jsx-boolean-value": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-no-bind": 0,
    "max-len": ["error", 160, 4, {"ignoreUrls": true}],
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "object-curly-spacing": 0,
    "prefer-template": 0
  }
}
