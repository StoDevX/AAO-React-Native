{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "scripts"
  ]
}
