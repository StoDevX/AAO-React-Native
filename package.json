{
  "name": "AllAboutOlaf",
  "version": "2.1.2",
  "private": true,
  "config": {
    "browserify": "--full-paths --debug --extension .web.js -t babelify -g [ babelify --ignore '\\/node_modules\\/(?!react-native)' ] -t aliasify -g aliasify -t imgurify -t loose-envify",
    "prettier_args": "--single-quote --trailing-comma all --no-bracket-spacing"
  },
  "scripts": {
    "android": "react-native run-android",
    "android-emulator": "VBoxManage list vms | peco --select-1 --query API --on-cancel error | sed  's~.*{\\(.*\\)}.*~\\1~' | xargs /Applications/Genymotion.app/Contents/MacOS/player.app/Contents/MacOS/player --vm-name",
    "bundle-data": "node scripts/bundle-data.js data/ docs/",
    "bundle:android": "react-native bundle --entry-file index.android.js --dev true --platform android --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res/",
    "bundle:ios": "react-native bundle --entry-file index.ios.js --dev true --platform ios --bundle-output ./ios/AllAboutOlaf/main.jsbundle --assets-dest ./ios",
    "bundle:web": "browserify $npm_package_config_browserify index.web.js -o ./web/bundle.web.js",
    "bundle:web:watch": "watchify -v $npm_package_config_browserify index.web.js -o ./web/bundle.web.js",
    "check": "npm run prettier && npm run flow && npm run test",
    "danger": "danger",
    "data": "node scripts/bundle-data.js data/ docs/",
    "flow": "flow",
    "ios": "react-native run-ios",
    "ios-simulator": "xcrun instruments -s devices | peco --select-1 --query 'Simulator iPhone' --on-cancel error | sed  's~.*\\[\\(.*\\)\\].*~\\1~' | xargs open -n -a Simulator --args -CurrentDeviceUDID",
    "lint": "eslint --cache source/ *.js",
    "prettier": "prettier --write $npm_package_config_prettier_args 'source/**/*.js' '*.js' && eslint --cache --fix 'source/' '*.js'",
    "prettier:changed": "FILES=$(git diff --name-only | grep '\\.js$'); prettier --write $npm_package_config_prettier_args $FILES; eslint --cache --fix $FILES",
    "prettier:since-master": "FILES=$(git diff --name-only master | grep '\\.js$'); prettier --write $npm_package_config_prettier_args $FILES; eslint --cache --fix $FILES",
    "serve": "echo open localhost:8080; cd web && python3 -m http.server 8080",
    "start": "react-native start",
    "web": "echo wait for the \\'... written to ...\\' message; npm run bundle:web:watch",
    "test": "jest",
    "validate-data": "node scripts/validate-data.js"
  },
  "codepush": {
    "ios": {
      "release": "oSozdf5RtDr2j8T7PFz-oWgNmoBIEJN_nzUOz",
      "staging": "wU8CTAoyYARqbOBVPXo6kWm7iq8CEJN_nzUOz",
      "nightly": "lkDky5WyEEILExg-RWX6ymfrJDn6EJN_nzUOz"
    },
    "android": {
      "release": "5jvlQ5-xBItoSRqf-D-3hqdEsPTLEJN_nzUOz",
      "staging": "GsN8Ii8enIpe8dVijiFQWZ743lV_EJN_nzUOz",
      "nightly": "cMzHZZ5aUuk0ILftVFFwJvZLxiycEJN_nzUOz"
    }
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.(test|spec)\\.js$",
    "collectCoverageFrom": [
      "source/**/*.js",
      "!**/node_modules/**"
    ],
    "setupFiles": [
      "react-native-mock"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "preset": "react-native"
  },
  "aliasify": {
    "aliases": {
      "react-native": "react-native-web",
      "react-native-keychain": false,
      "react-native-view-shot": false,
      "react-native-safari-view": false
    }
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "buffer": "5.0.5",
    "css-select": "1.2.0",
    "dedent": "0.7.0",
    "delay": "2.0.0",
    "events": "1.1.1",
    "html-entities": "1.2.0",
    "htmlparser2": "3.9.2",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "moment-timezone": "0.5.11",
    "querystring": "0.2.0",
    "react": "15.4.2",
    "react-addons-shallow-compare": "15.4.2",
    "react-native": "0.42.3",
    "react-native-alphabetlistview": "github:sunnylqm/react-native-alphabetlistview#aea5d61fdd9034c7178d73ce8b2ee104da6941cc",
    "react-native-button": "1.8.2",
    "react-native-code-push": "1.17.2-beta",
    "react-native-communications": "2.2.1",
    "react-native-custom-tabs": "0.1.3",
    "react-native-device-info": "0.10.1",
    "react-native-google-analytics-bridge": "5.0.1",
    "react-native-keychain": "1.1.0",
    "react-native-linear-gradient": "2.0.0",
    "react-native-onesignal": "3.0.3",
    "react-native-parallax-view": "2.0.6",
    "react-native-safari-view": "2.0.0",
    "react-native-sortable-list": "github:hawkrives/react-native-sortable-list#a46c78161e0a1a8d3a45f2fef69d98af3e911b6d",
    "react-native-tab-view": "0.0.57",
    "react-native-tableview-simple": "0.16.4",
    "react-native-vector-icons": "4.0.0",
    "react-native-web": "0.0.77",
    "react-redux": "5.0.3",
    "redux": "3.6.0",
    "redux-logger": "3.0.0",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "titlecase": "1.1.2",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "ajv": "4.11.5",
    "aliasify": "2.1.0",
    "babel-eslint": "7.2.1",
    "babel-jest": "18.0.0",
    "babel-plugin-flow-react-proptypes": "0.21.0",
    "babel-preset-react-native": "1.9.1",
    "babelify": "7.3.0",
    "browserify": "14.1.0",
    "danger": "0.14.1",
    "enzyme": "2.7.1",
    "enzyme-to-json": "1.5.0",
    "eslint": "3.18.0",
    "eslint-config-prettier": "1.4.1",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-flowtype": "2.30.4",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-react-native": "2.3.1",
    "flow-bin": "0.38.0",
    "imgurify": "2.0.0",
    "jest": "18.1.0",
    "js-yaml": "3.8.1",
    "junk": "2.1.0",
    "loose-envify": "1.3.1",
    "marked": "0.3.6",
    "minimist": "1.2.0",
    "pify": "2.3.0",
    "prettier": "0.22.0",
    "react-native-mock": "0.3.1",
    "react-native-view-shot": "1.9.1",
    "react-test-renderer": "15.4.2",
    "watchify": "3.9.0"
  }
}
