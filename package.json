{
  "name": "AllAboutOlaf",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "rn": "react-native",
    "lint": "eslint app.js *.index.js lib/ views/",
    "android": "react-native run-android",
    "android-emulator": "emulator -avd 'react-native'",
    "flow": "flow",
    "bundle:ios": "react-native bundle --entry-file index.ios.js --dev true --platform ios --bundle-output ./ios/AllAboutOlaf/main.jsbundle --assets-dest ./ios",
    "bundle:android": "react-native bundle --entry-file index.android.js --dev true --platform android --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build:ios": "xcodebuild -configuration Release -workspace ios/AllAboutOlaf.xcworkspace -scheme AllAboutOlaf -sdk iphonesimulator -destination platform='iOS Simulator,name=iPhone 5s,OS=9.2' | xcpretty",
    "build:android:unix": "cd android && ./gradlew assembleRelease --info --console=plain",
    "build:android:win": "cd android && .\\gradlew assembleRelease --info --console=plain",
    "test": "npm run flow && npm run test:js",
    "test:js": "jest",
    "test:js:coverage": "jest --coverage",
    "test:js:watch": "jest --watch",
    "test:ios": "xctool -configuration Release -workspace ios/AllAboutOlaf.xcworkspace -scheme AllAboutOlaf -sdk iphonesimulator -destination platform='iOS Simulator,name=iPhone 5s,OS=9.2' test",
    "test:android": "echo 'Not yet implemented! See http://facebook.github.io/react-native/docs/signed-apk-android.html#testing-the-release-build-of-your-app'",
    "release": "npm run release:ios && npm run release:android",
    "release:ios": "echo 'Not yet implemented!'",
    "release:android": "echo 'Not yet implemented!'"
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.(test|spec)\\.js$",
    "collectCoverageFrom": [
      "{lib,views}/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "preset": "jest-react-native"
  },
  "dependencies": {
    "buffer": "5.0.1",
    "css-select": "1.2.0",
    "delay": "1.3.1",
    "events": "1.1.1",
    "frisbee": "1.1.6",
    "html-entities": "1.2.0",
    "htmlparser2": "3.9.2",
    "lodash": "4.17.2",
    "moment": "2.17.0",
    "moment-timezone": "0.5.10",
    "querystring": "0.2.0",
    "react": "15.4.1",
    "react-addons-shallow-compare": "15.4.1",
    "react-native": "0.38.0",
    "react-native-alphabetlistview": "0.2.0",
    "react-native-button": "1.7.1",
    "react-native-communications": "2.1.3",
    "react-native-cookies": "2.0.0",
    "react-native-device-info": "0.9.6",
    "react-native-keychain": "0.3.2",
    "react-native-onesignal": "1.2.3",
    "react-native-refreshable-listview": "1.3.0",
    "react-native-tab-view": "0.0.40",
    "react-native-tableview-simple": "0.13.0",
    "react-native-vector-icons": "3.0.0",
    "redux": "3.6.0",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-preset-react-native": "1.9.0",
    "eslint": "3.11.1",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-flowtype": "2.28.2",
    "eslint-plugin-react": "6.7.1",
    "flow-bin": "0.35.0",
    "jest": "17.0.2",
    "jest-react-native": "17.0.2",
    "react-test-renderer": "15.4.1"
  }
}
