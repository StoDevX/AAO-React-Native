{
  "name": "AllAboutOlaf",
  "version": "2.8.0-beta.4",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "bundle-data": "node scripts/bundle-data.js data/ docs/",
    "bundle:android": "react-native bundle --entry-file index.js --dev true --platform android --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res/ --sourcemap-output ./android/app/src/main/assets/index.android.bundle.map",
    "bundle:ios": "react-native bundle --entry-file index.js --dev false --platform ios --bundle-output ./ios/AllAboutOlaf/main.jsbundle --assets-dest ./ios --sourcemap-output ./ios/AllAboutOlaf/main.jsbundle.map",
    "compress-data": "gzip --keep docs/*.json",
    "danger": "danger ci",
    "data": "node scripts/bundle-data.js data/ docs/",
    "d": "yarn-deduplicate && yarn",
    "flow": "flow",
    "ios": "react-native run-ios",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache source/ modules/ scripts/ *.js",
    "prepare": "ls contrib/*.patch | xargs -n1 patch -p0 -Nfsi || true",
    "pretty": "prettier --write '{source,modules,scripts,e2e}/**/*.{js,json}' 'data/**/*.css' '{*,.*}.{yaml,yml,json,js}' '.circleci/*'",
    "p": "pretty-quick",
    "postinstall": "jetify",
    "start": "react-native start",
    "test": "jest",
    "validate-bus-data": "node scripts/validate-bus-schedules.js",
    "validate-data": "node scripts/validate-data.js"
  },
  "workspaces": {
    "packages": [
      "modules/*"
    ]
  },
  "resolutions": {
    "react-navigation/@react-navigation/native/react-native-screens": "^2.0.0-alpha.1"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.(spec|test).(js|ts|tsx)",
      "!**/node_modules/**"
    ],
    "collectCoverageFrom": [
      "modules/**/*.js",
      "source/**/*.js",
      "!**/node_modules/**"
    ],
    "setupFiles": [
      "./scripts/jest-setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|glamorous-native|react-navigation)"
    ],
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/AllAboutOlaf.app",
        "build": "xcodebuild -project ios/AllAboutOlaf.xcodeproj -scheme AllAboutOlaf -configuration Debug -destination 'platform=iOS Simulator,name=iPhone 7,OS=12.2' -derivedDataPath ios/build build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/AllAboutOlaf.app",
        "build": "xcodebuild -project ios/AllAboutOlaf.xcodeproj -scheme AllAboutOlaf -configuration Release -destination 'platform=iOS Simulator,name=iPhone 7,OS=12.2' -derivedDataPath ios/build build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "specs": "",
    "test-runner": "jest"
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "semi": false,
    "overrides": [
      {
        "files": "images/**",
        "options": {
          "requirePragma": true
        }
      },
      {
        "files": "**/{*,.*}.json",
        "options": {
          "parser": "json-stringify",
          "useTabs": false,
          "tabWidth": 2
        }
      }
    ]
  },
  "dependencies": {
    "@callstack/react-theme-provider": "3.0.3",
    "@frogpond/titlecase": "1.0.0",
    "@hawkrives/react-native-alternate-icons": "0.4.7",
    "@mapbox/react-native-mapbox-gl": "6.1.3",
    "@react-native-community/netinfo": "4.2.1",
    "base-64": "0.1.0",
    "buffer": "5.4.2",
    "delay": "4.3.0",
    "events": "3.0.0",
    "glamorous-native": "2.0.0",
    "jsc-android": "241213.1.0",
    "keyword-search": "0.1.2",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "moment-timezone": "0.5.26",
    "p-props": "3.1.0",
    "p-retry": "4.1.0",
    "query-string": "6.8.3",
    "querystring": "0.2.0",
    "react": "16.8.6",
    "react-async": "8.0.0",
    "react-markdown": "2.5.1",
    "react-native": "0.60.5",
    "react-native-button": "2.4.0",
    "react-native-calendar-events": "1.7.3",
    "react-native-communications": "2.2.1",
    "react-native-custom-tabs": "0.1.7",
    "react-native-device-info": "2.3.2",
    "react-native-gesture-handler": "1.4.1",
    "react-native-keychain": "3.1.3",
    "react-native-linear-gradient": "2.5.6",
    "react-native-location": "2.5.0",
    "react-native-network-info": "5.1.0",
    "react-native-paper": "2.16.0",
    "react-native-popover-view": "1.0.19",
    "react-native-restart": "0.0.13",
    "react-native-safari-view": "2.1.0",
    "react-native-screens": "2.0.0-alpha.2",
    "react-native-search-bar": "3.4.3",
    "react-native-sentry": "0.43.2",
    "react-native-tableview-simple": "2.3.0",
    "react-native-typography": "1.4.1",
    "react-native-vector-icons": "6.6.0",
    "react-native-webview": "7.0.4",
    "react-navigation": "3.12.1",
    "react-navigation-material-bottom-tabs": "1.1.1",
    "react-redux": "7.1.1",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-promise": "0.6.0",
    "redux-thunk": "2.3.0",
    "semver": "6.3.0",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "tinycolor2": "1.4.1",
    "url": "0.11.0",
    "wordwrap": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/runtime": "7.6.0",
    "@sentry/cli": "1.47.1",
    "ajv": "6.10.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "danger": "9.2.0",
    "danger-plugin-yarn": "1.3.2",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.2.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "eslint-plugin-react-native": "3.7.0",
    "flow-bin": "0.98.1",
    "jest": "24.9.0",
    "jetifier": "1.6.4",
    "js-yaml": "3.13.1",
    "junk": "3.1.0",
    "metro-react-native-babel-preset": "0.56.0",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "pify": "4.0.1",
    "prettier": "1.18.2",
    "pretty-bytes": "5.3.0",
    "pretty-quick": "1.11.1",
    "prop-types": "15.7.2",
    "react-test-renderer": "16.8.6",
    "string-natural-compare": "2.0.3",
    "strip-ansi": "5.2.0",
    "yarn-deduplicate": "1.1.1"
  },
  "optionalDependencies": {
    "detox": "14.2.0"
  }
}
