{
  "name": "all-about-olaf",
  "version": "2.4.2-beta.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "android-emulator": "VBoxManage list vms | peco --select-1 --query API --on-cancel error | sed  's~.*{\\(.*\\)}.*~\\1~' | xargs /Applications/Genymotion.app/Contents/MacOS/player.app/Contents/MacOS/player --vm-name",
    "bundle-data": "node scripts/bundle-data.js data/ docs/",
    "bundle:android": "react-native bundle --entry-file index.android.js --dev true --platform android --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res/",
    "bundle:ios": "react-native bundle --entry-file index.ios.js --dev true --platform ios --bundle-output ./ios/AllAboutOlaf/main.jsbundle --assets-dest ./ios",
    "check": "npm run prettier && npm run flow && npm run validate-data -- --quiet && npm run test",
    "danger": "danger",
    "data": "node scripts/bundle-data.js data/ docs/",
    "flow": "flow",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --configuration Release",
    "ios-simulator": "xcrun instruments -s devices | peco --select-1 --query 'Simulator iPhone' --on-cancel error | sed  's~.*\\[\\(.*\\)\\].*~\\1~' | xargs open -n -a Simulator --args -CurrentDeviceUDID",
    "lint": "eslint --cache source/ *.js",
    "prettier": "prettier --write '{source,scripts}/**/*.js' 'data/**/*.css' '*.js'",
    "prettier:changed": "FILES=$(git status -u --porcelain | cut -c4- | grep '\\.js$'); prettier --write $FILES",
    "prettier:since-master": "FILES=$(git diff --name-only master | grep '\\.js$'); prettier --write $FILES",
    "start": "react-native start",
    "test": "jest",
    "validate-data": "node scripts/validate-data.js"
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "flow-bin"
    ]
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.(test|spec)\\.js$",
    "collectCoverageFrom": [
      "source/**/*.js",
      "!**/node_modules/**"
    ],
    "preset": "react-native"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "semi": false
  },
  "dependencies": {
    "@hawkrives/react-native-alphabetlistview": "1.0.0",
    "@hawkrives/react-native-search-bar": "3.0.0-3",
    "@hawkrives/react-native-sortable-list": "1.0.1",
    "buffer": "5.0.8",
    "bugsnag-react-native": "2.5.1",
    "css-select": "1.2.0",
    "delay": "2.0.0",
    "events": "1.1.1",
    "get-urls": "7.0.0",
    "glamorous-native": "1.2.0",
    "html-entities": "1.2.1",
    "htmlparser2": "3.9.2",
    "lodash": "4.17.4",
    "moment": "2.19.1",
    "moment-timezone": "0.5.14",
    "querystring": "0.2.0",
    "react": "16.0.0-alpha.12",
    "react-markdown": "2.5.0",
    "react-native": "0.45.1",
    "react-native-button": "2.1.0",
    "react-native-calendar-events": "1.4.1",
    "react-native-communications": "2.2.1",
    "react-native-custom-tabs": "0.1.7",
    "react-native-device-info": "0.12.1",
    "react-native-google-analytics-bridge": "5.3.3",
    "react-native-keychain": "2.0.0-rc",
    "react-native-linear-gradient": "2.3.0",
    "react-native-maps": "0.17.1",
    "react-native-network-info": "2.2.0",
    "react-native-onesignal": "3.0.6",
    "react-native-restart": "0.0.4",
    "react-native-safari-view": "2.0.0",
    "react-native-tableview-simple": "0.17.1",
    "react-native-vector-icons": "4.4.2",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "titlecase": "1.1.2",
    "url": "0.11.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "ajv": "5.3.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "3.0.1",
    "bugsnag-sourcemaps": "1.0.1",
    "danger": "2.0.1",
    "eslint": "4.11.0",
    "eslint-config-prettier": "2.7.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-react-native": "3.1.0",
    "flow-bin": "0.45.0",
    "jest": "21.2.1",
    "js-yaml": "3.10.0",
    "junk": "2.1.0",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "pify": "3.0.0",
    "prettier": "1.8.2",
    "prop-types": "15.6.0",
    "react-test-renderer": "16.0.0-alpha.12",
    "string-natural-compare": "2.0.2"
  }
}
