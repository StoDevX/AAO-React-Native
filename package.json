{
  "name": "all-about-olaf",
  "version": "2.4.0-beta.16",
  "private": true,
  "config": {
    "prettier_args": "--single-quote --trailing-comma all --no-bracket-spacing --no-semi"
  },
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "android-emulator": "VBoxManage list vms | peco --select-1 --query API --on-cancel error | sed  's~.*{\\(.*\\)}.*~\\1~' | xargs /Applications/Genymotion.app/Contents/MacOS/player.app/Contents/MacOS/player --vm-name",
    "bundle-data": "node scripts/bundle-data.js data/ docs/",
    "bundle:android": "react-native bundle --entry-file index.android.js --dev true --platform android --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res/",
    "bundle:ios": "react-native bundle --entry-file index.ios.js --dev true --platform ios --bundle-output ./ios/AllAboutOlaf/main.jsbundle --assets-dest ./ios",
    "check": "npm run prettier && npm run flow && npm run validate-data -- --quiet && npm run test",
    "danger": "danger",
    "data": "node scripts/bundle-data.js data/ docs/",
    "flow": "flow",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --configuration Release",
    "ios-simulator": "xcrun instruments -s devices | peco --select-1 --query 'Simulator iPhone' --on-cancel error | sed  's~.*\\[\\(.*\\)\\].*~\\1~' | xargs open -n -a Simulator --args -CurrentDeviceUDID",
    "lint": "eslint --cache source/ *.js",
    "prettier": "prettier --write $npm_package_config_prettier_args 'source/**/*.js' 'data/**/*.css' '*.js'",
    "prettier:changed": "FILES=$(git status -u --porcelain | cut -c4- | grep '\\.js$'); prettier --write $npm_package_config_prettier_args $FILES",
    "prettier:since-master": "FILES=$(git diff --name-only master | grep '\\.js$'); prettier --write $npm_package_config_prettier_args $FILES",
    "start": "react-native start",
    "test": "jest",
    "validate-data": "node scripts/validate-data.js"
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "flow-bin"
    ]
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.(test|spec)\\.js$",
    "collectCoverageFrom": [
      "source/**/*.js",
      "!**/node_modules/**"
    ],
    "preset": "react-native"
  },
  "dependencies": {
    "buffer": "5.0.7",
    "bugsnag-react-native": "2.3.2",
    "css-select": "1.2.0",
    "dedent": "0.7.0",
    "delay": "2.0.0",
    "events": "1.1.1",
    "get-urls": "7.0.0",
    "glamorous-native": "1.1.2",
    "html-entities": "1.2.1",
    "htmlparser2": "3.9.2",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "moment-timezone": "0.5.13",
    "querystring": "0.2.0",
    "react": "16.0.0",
    "react-addons-shallow-compare": "15.6.2",
    "react-markdown": "2.5.0",
    "react-native": "0.45.1",
    "react-native-alphabetlistview": "github:sunnylqm/react-native-alphabetlistview#aea5d61fdd9034c7178d73ce8b2ee104da6941cc",
    "react-native-button": "2.1.0",
    "react-native-calendar-events": "1.4.1",
    "react-native-communications": "2.2.1",
    "react-native-custom-tabs": "0.1.7",
    "react-native-datepicker": "1.6.0",
    "react-native-device-info": "0.11.0",
    "react-native-google-analytics-bridge": "5.3.3",
    "react-native-keychain": "2.0.0-rc",
    "react-native-linear-gradient": "2.3.0",
    "react-native-maps": "0.16.4",
    "react-native-network-info": "2.2.0",
    "react-native-onesignal": "3.0.5",
    "react-native-restart": "0.0.3",
    "react-native-safari-view": "2.0.0",
    "react-native-search-bar": "3.0.0",
    "react-native-sortable-list": "github:hawkrives/react-native-sortable-list#7b72cc8e16cc71751836b49b3ea4bbc9e8e9cb99",
    "react-native-tableview-simple": "0.16.12",
    "react-native-vector-icons": "4.4.0",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "titlecase": "1.1.2",
    "url": "0.11.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "ajv": "5.2.3",
    "babel-eslint": "8.0.0",
    "babel-jest": "21.0.2",
    "babel-plugin-flow-react-proptypes": "5.2.0",
    "babel-preset-react-native": "3.0.1",
    "danger": "2.0.0-alpha.15",
    "eslint": "4.7.2",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.36.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-react-native": "3.1.0",
    "flow-bin": "0.45.0",
    "jest": "21.1.0",
    "js-yaml": "3.10.0",
    "junk": "2.1.0",
    "marked": "0.3.6",
    "minimist": "1.2.0",
    "pify": "3.0.0",
    "prettier": "1.4.4",
    "react-native-view-shot": "1.12.0",
    "react-test-renderer": "16.0.0-alpha.12",
    "string-natural-compare": "2.0.2"
  }
}
