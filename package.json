{
  "name": "all-about-olaf",
  "version": "2.8.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "bundle-data": "node scripts/bundle-data.mjs data/ docs/",
    "bundle:android": "mkdir -p android/generated/assets/ android/generated/res/ && react-native bundle --entry-file index.js --dev true --platform android --bundle-output ./android/generated/assets/index.android.bundle  --sourcemap-output ./android/generated/assets/index.android.bundle.map --assets-dest ./android/generated/res/",
    "bundle:ios": "react-native bundle --entry-file index.js --dev false --platform ios --bundle-output ./ios/AllAboutOlaf/main.jsbundle --sourcemap-output ./ios/AllAboutOlaf/main.jsbundle.map --assets-dest ./ios",
    "fastlane": "bundle exec fastlane",
    "compress-data": "gzip --keep docs/*.json",
    "data": "echo 'use the `bundle-data` script instead' && exit 1",
    "ios": "react-native run-ios",
    "lint": "eslint --report-unused-disable-directives --cache source/ modules/ scripts/ images/",
    "prepare": "patch -p0 -Nfsi contrib/*.patch || true",
    "pretty": "prettier --write '{source,modules,scripts,images,e2e}/**/*.{js,ts,tsx,json}' 'data/**/*.css' '{*,.*}.{yaml,yml,json,json5,js,ts,tsx}'",
    "p": "pretty-quick",
    "pods": "scripts/pods.sh",
    "start": "react-native start",
    "test": "jest",
    "validate-bus-data": "node scripts/validate-bus-schedules.mjs",
    "validate-data": "node scripts/validate-data.mjs"
  },
  "workspaces": {
    "packages": [
      "modules/*"
    ]
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "semi": false,
    "overrides": [
      {
        "files": "**/{*,.*}.json",
        "options": {
          "parser": "json-stringify",
          "useTabs": false,
          "tabWidth": 2
        }
      }
    ]
  },
